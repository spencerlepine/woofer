    "@shelf/jest-mongodb": "^2.2.1",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.4",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.5",
    "supertest": "^6.2.2"
