{
  "name": "woofer-server",
  "version": "1.0.0",
  "description": "Dating app for dogs, a full stack MERN project",
  "author": "Spencer Lepine",
  "repository": {
    "type": "git",
    "url": "https://github.com/spencerlepine/woofer"
  },
  "keywords": [
    "node",
    "express",
    "mongodb"
  ],
  "license": "MIT",
  "scripts": {
    "start": "node src/index.js",
    "dev:server": "nodemon src/index.js",
    "report-coverage": "npm run test -- --coverage && codecov",
    "test": "jest --detectOpenHandles --runInBand --forceExit",
    "test:watch": "jest --onlyChanged --watch"
  },
  "engines": {
    "node": "16.9.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.21.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.3.0",
    "firebase": "^8.10.1",
    "helmet": "^5.0.2",
    "http-status": "^1.5.0",
    "joi": "^17.6.0",
    "mongodb-memory-server-core": "^8.5.2",
    "mongoose": "^6.2.10",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "puppeteer": "^13.7.0",
    "socket.io": "^4.4.1",
