{
  "name": "woofer",
  "version": "1.0.0",
  "description": "Dating app for dogs, a full stack MERN project",
  "author": "Spencer Lepine",
  "repository": {
    "type": "git",
    "url": "https://github.com/spencerlepine/woofer"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "mongodb"
  ],
  "license": "MIT",
  "scripts": {
    "start": "node server/src/index.js",
    "dev:server": "nodemon server/src/index.js",
    "dev:client": "webpack serve --config ./webpack.dev.config.js --env development --mode development",
    "build": "webpack --config ./webpack.prod.config.js --env production --mode production",
    "lint": "eslint . --ext .js",
    "test": "jest --detectOpenHandles --runInBand --forceExit",
    "test:watch": "jest --onlyChanged --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
